@model Mailings.Web.ViewModels.MailingViewModel
<div class="action-block">
    <a class="button"
       asp-action="More" asp-controller="Mailings"
       asp-route-id="@Model.Id">
        More...
    </a>
</div>
<list struct="Text">
    <el>Name: @Model.Name</el>
    <el>Mail theme: @Model.Mail.Theme</el>
    <el>Mail type: @Model.Mail.Type</el>
    <el>To emails: @ViewBag.FormattedSenders</el>
</list>